{
    "version": 2,
    "builds": [
      { "src": "frontend/package.json", "use": "@vercel/static-build" },
      { "src": "api/submit-form.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/submit-form.js" },
      { "src": "/(.*)", "dest": "/frontend/$1" }
    ]
  }